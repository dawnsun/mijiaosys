<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>
		<typeAlias alias="SysUser" type="com.mijiaokj.sys.domain.SysUser" />
		<typeAlias alias="ControlFileRelation" type="com.mijiaokj.sys.domain.ControlFileRelation" />
		<typeAlias alias="MenuMehtedRelation" type="com.mijiaokj.sys.domain.MenuMehtedRelation" />
		<typeAlias alias="RoleResRelation" type="com.mijiaokj.sys.domain.RoleResRelation" />
		<typeAlias alias="RoleControl" type="com.mijiaokj.sys.domain.RoleControl" />
		<typeAlias alias="SysFile" type="com.mijiaokj.sys.domain.SysFile" />
		<typeAlias alias="ResMenu" type="com.mijiaokj.sys.domain.ResMenu" />
		<typeAlias alias="ResMethed" type="com.mijiaokj.sys.domain.ResMethed" />
		<typeAlias alias="SysInfo" type="com.mijiaokj.sys.domain.SysInfo" />
		<typeAlias alias="SysRole" type="com.mijiaokj.sys.domain.SysRole" />
		<typeAlias alias="SysRoleRelation" type="com.mijiaokj.sys.domain.SysRoleRelation" />
		<typeAlias alias="SysUserRoleRelation" type="com.mijiaokj.sys.domain.SysUserRoleRelation" />
		<typeAlias alias="UserSysRelation" type="com.mijiaokj.sys.domain.UserSysRelation" />
	</typeAliases>
	
</configuration>
